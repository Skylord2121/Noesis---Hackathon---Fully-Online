{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "webapp",
  "compatibility_date": "2025-10-14",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "kv_namespaces": [
    {
      "binding": "SESSIONS",
      "id": "placeholder",
      "preview_id": "placeholder"
    }
  ],
  // Environment variables - loaded from .dev.vars file
  // For production, set OLLAMA_URL via: wrangler secret put OLLAMA_URL
  "vars": {
    "OLLAMA_URL": "http://localhost:11434"
  }
